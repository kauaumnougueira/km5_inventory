<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.layoutController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="add" layoutX="220.0" layoutY="115.0" mnemonicParsing="false" onAction="#onChangeActionAdd" prefHeight="37.0" prefWidth="201.0" text="ADCIONAR UM SERVIÇO" />
            <Label layoutX="164.0" layoutY="14.0" prefHeight="47.0" prefWidth="312.0" text="GERENCIADOR DE CLIENTES E SERVIÇOS DA KM5">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="toSee" layoutX="220.0" layoutY="194.0" mnemonicParsing="false" onAction="#onChangeActionToSee" prefHeight="37.0" prefWidth="201.0" text="VER TABELA" />
            <Button fx:id="change" layoutX="220.0" layoutY="271.0" mnemonicParsing="false" onAction="#onChangeActionChange" prefHeight="37.0" prefWidth="201.0" text="ALTERAR TABELA" />
      </children>
    </AnchorPane>
  </children>
</VBox>
